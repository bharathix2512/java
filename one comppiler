 1.import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      int a=s.nextInt();
      int b=s.nextInt();
      if(a<b){
        System.out.println(b);
        
      }
      else{
        System.out.println(a);
      }
        
      
  }
}


2.import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      int a = s.nextInt();
      if(a>0){
        System.out.println("POSITIVE");
      }
      else if(a<0){
        System.out.println("NEGATIVE");
      }
      else{
        System.out.println("ZERO");
      }
  }
}



