6.import java.util.*;

public class Main {
    public static void main(String[] args) {
     Scanner s=new Scanner(System.in);
      int a = s.nextInt();
      int b=s.nextInt();
      int c=s.nextInt();
      if(a>b&&a>c){
        System.out.println(+a);
      }
      else if(b>a&&b>c){
        System.out.println(+b);
      }
      else{
        System.out.println(+c);
      }
  }
}


7.import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      
            int a=s.nextInt();
            int b=s.nextInt();
      int c=s.nextInt();
      int d=s.nextInt();
    


      if (a>b){
        if(a>c){
          if (a>d)
          System.out.println(a);
          else
            System.out.println(d);
        }
        else
          System.out.println(c);
      }
      else if (b>c){
        if (b>d)
          System.out.println(b);
        else
          System.out.println(d);
      }
      else if (c>d){
        System.out.println(c);
      }
      else
        System.out.println(d);
  }
}
  
