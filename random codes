1.prime number or not

import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner s=new Scanner (System.in);
        int a=s.nextInt();
        int c=0;
        for(int i=1;i<=a;i++){
            if(a%i==0){
                c++;
                
        }
        }
        if(c==2){
            System.out.println("PRIME");
        }
        else{
            System.out.println("NOT A PRIME");
        }
    
}
}


